<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<!-- namespace속성 : 아래에서 작성한 SQL을 호출할 때 같이 사용된다. -->			
<sqlMap namespace="zipcodeSqlMap">
	<select id="selectSearch" resultClass="zipVO" parameterClass="zipVO">
		select * from ziptb
		<dynamic prepend="where">
			<isNotEmpty property="dong" prepend="or">
				dong like '%' || #dong# || '%'
			</isNotEmpty>
			<isNotEmpty property="zipcode" prepend="or">
				zipcode like '%' || #zipcode# || '%'
			</isNotEmpty>
		</dynamic>
	</select>
</sqlMap>